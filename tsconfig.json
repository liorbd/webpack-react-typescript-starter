{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017",
      "ESNext"
    ],
    "sourceMap": true,
    "declaration": true,
    "allowJs": false,
    "jsx": "react",
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": false
  },
  "exclude": [
    "node_modules"
  ]
}